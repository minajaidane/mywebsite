{% if collections.work | highlighted | length %}
    <section class= "container">
        <h2>My work experience</h2>
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 42 42"><path d="M0 20h42v2H0z"/></svg>
        {% if collections.work | highlighted %}
            <ul class="articles row">
                {% for article in collections.work | highlighted %}
                    {% if article.url != page.url %}
                      <li class="work col-12 col-md-8 col-lg-5 design__card">
            <a href="{{ article.url | url }}">
              <img class="work__image" src="{{ '/static/images/work/' | url }}{{ article.data.img }}" alt="{{article.data.title}}" />
              <h3>{{ article.data.client }}</h3>
              <div class = "work__text">
                 <p class = "work__client">{{article.data.title}}</p>
                <p class = "work__CTA btn btn-primary">Find out more!</p></a>
              </div>

            </a>
                    {% endif %}
                {% endfor %}
            </ul>
        {% endif %}
    </section>
  {% endif %}


  